        let comboboxs = $('.mcombobox');
        for (const combobox of comboboxs) {
            let itemDataElements = $(combobox).find('.m-combobox-data').html();
            $(combobox).data("itemDataElement ", itemDataElements);
            // $(combobox).find('.m-combobox-data').empty();

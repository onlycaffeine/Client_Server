                || validateward == null
                || input_name_guardian == ""
                || input_name_guardian == null
                || input_relation == ""
                || input_relation == null
                || input_phone_guardian == ""
                || input_phone_guardian == null
                || ngaytiem1 == ""
                || ngaytiem1 == null
                || tenvc1 == ""
                || tenvc1 == null) {
                document.getElementById('toatmessage1').style.display = 'flex';
                setTimeout(show, 3000);
            }
            else {
                let a = document.getElementById("dateof").value;
                var date = new Date(a);
                if (date.getDate() > 0) {
                    var today = new Date();
                    var year = date.getFullYear();
                    var month = date.getMonth();
                    var day = date.getDay();
                    var age = today.getFullYear() - year;
                    if (today.getMonth() < month || (today.getMonth() == month && today.getDate() < day)) {
                        age--;
                    }

                    if (age <= 12) {
                        document.getElementById('toatmessage').style.display = 'flex';
                        setTimeout(check, 3000);
                    }
                    else {
                        document.getElementById("m-dialog-modal").style.display = "block";
                        document.getElementById("content").style.display = "none";
                        document.documentElement.scrollTop = 0;
                        document.getElementById("step2").classList.add("content-steps-1");
                        document.getElementById("step1").classList.remove("content-steps-1");
                    }
                }
            }
        }
        //trường hợp còn lại
        else {

            document.getElementById('loaimui').focus();
            document.documentElement.scrollTop = 0;
            document.getElementById('toatmessage2').style.display = 'flex';
            setTimeout(show2, 3000);
        }
    }

